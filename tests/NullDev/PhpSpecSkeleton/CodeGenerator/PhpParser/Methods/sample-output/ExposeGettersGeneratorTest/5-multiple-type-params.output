<?php

public function it_should_expose_constructor_arguments_via_getters()
{
    $this->getId()->shouldReturn(1);
    $this->getName()->shouldReturn('name');
    $this->getPrice()->shouldReturn(2.0);
    $this->getSmart()->shouldReturn(true);
    $this->getTags()->shouldReturn([]);
    $this->getRandomValue()->shouldReturn('randomValue');
}