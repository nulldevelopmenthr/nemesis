<?php

namespace tests\MyCompany\Entity;

use Mockery;
use Mockery\Adapter\Phpunit\MockeryPHPUnitIntegration;
use MyCompany\Entity\TransactionEntity;
use PHPUnit\Framework\TestCase;
/**
* @covers \MyCompany\Entity\TransactionEntity
* @group todo
*/
class TransactionEntityTest extends TestCase
{
    use MockeryPHPUnitIntegration;
    /** @var TransactionEntity */
    private $transactionEntity;
    public function setUp()
    {
        $this->transactionEntity = new TransactionEntity();
    }
    public function testId()
    {
        $id = 'id';
        $this->transactionEntity->setId($id);
        self::assertEquals($id, $this->transactionEntity->getId());
    }
    public function testAmount()
    {
        $amount = 1;
        $this->transactionEntity->setAmount($amount);
        self::assertEquals($amount, $this->transactionEntity->getAmount());
    }
    public function testCreatedAt()
    {
        $createdAt = new \DateTime();
        $this->transactionEntity->setCreatedAt($createdAt);
        self::assertEquals($createdAt, $this->transactionEntity->getCreatedAt());
    }
}