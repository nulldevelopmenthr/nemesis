<?php

namespace tests\Developer;

use Developer\Senior;
use FirstName;
use Mockery;
use Mockery\Adapter\Phpunit\MockeryPHPUnitIntegration;
use Mockery\MockInterface;
use PHPUnit_Framework_TestCase;
/**
* @covers \Developer\Senior
* @group todo
*/
class SeniorTest extends PHPUnit_Framework_TestCase
{
    use MockeryPHPUnitIntegration;
    /** @var MockInterface|FirstName */
    private $firstName;
    /** @var Senior */
    private $senior;
    public function setUp()
    {
        $this->firstName = Mockery::mock(FirstName::class);
        $this->senior = new Senior($this->firstName);
    }
    public function testGetFirstName()
    {
        self::assertEquals($this->firstName, $this->senior->getFirstName());
    }
}