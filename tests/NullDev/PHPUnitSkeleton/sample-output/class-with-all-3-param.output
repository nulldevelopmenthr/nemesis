<?php

namespace tests\Developer;

use Developer\Senior;
use FirstName;
use HR\Finances\Wage;
use LastName;
use Mockery;
use Mockery\Adapter\Phpunit\MockeryPHPUnitIntegration;
use Mockery\MockInterface;
use PHPUnit_Framework_TestCase;
/**
* @covers \Developer\Senior
* @group todo
*/
class SeniorTest extends PHPUnit_Framework_TestCase
{
    use MockeryPHPUnitIntegration;
    /** @var MockInterface|FirstName */
    private $firstName;
    /** @var MockInterface|LastName */
    private $lastName;
    /** @var MockInterface|Wage */
    private $amount;
    /** @var Senior */
    private $senior;
    public function setUp()
    {
        $this->firstName = Mockery::mock(FirstName::class);
        $this->lastName = Mockery::mock(LastName::class);
        $this->amount = Mockery::mock(Wage::class);
        $this->senior = new Senior($this->firstName, $this->lastName, $this->amount);
    }
    public function testGetFirstName()
    {
        self::assertEquals($this->firstName, $this->senior->getFirstName());
    }
    public function testGetLastName()
    {
        self::assertEquals($this->lastName, $this->senior->getLastName());
    }
    public function testGetAmount()
    {
        self::assertEquals($this->amount, $this->senior->getAmount());
    }
}