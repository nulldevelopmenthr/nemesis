<?php

namespace spec\Something;

use PhpSpec\ObjectBehavior;
use Something\CreateUserCommand;

class CreateUserCommandSpec extends ObjectBehavior
{
    public function let()
    {
        $this->beConstructedWith($id = 'id');
    }

    public function it_is_initializable()
    {
        $this->shouldHaveType(CreateUserCommand::class);
    }

    public function it_should_expose_constructor_arguments_via_getters()
    {
        $this->getId()->shouldReturn('id');
    }
}
