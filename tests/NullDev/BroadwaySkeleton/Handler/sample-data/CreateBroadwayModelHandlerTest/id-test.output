<?php

namespace tests\Something;

use Mockery;
use PHPUnit_Framework_TestCase;
use Something\UserId;
/**
* @covers \Something\UserId
* @group todo
*/
class UserIdTest extends PHPUnit_Framework_TestCase
{
    /** @var string */
    private $id;
    /** @var UserId */
    private $userId;
    public function setUp()
    {
        $this->id = 'id';
        $this->userId = new UserId($this->id);
    }
    public function testGetId()
    {
        self::assertEquals($this->id, $this->userId->getId());
    }
}