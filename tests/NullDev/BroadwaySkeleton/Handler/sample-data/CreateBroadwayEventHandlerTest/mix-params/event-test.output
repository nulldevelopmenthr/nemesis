<?php

namespace tests\Something;

use Mockery;
use PHPUnit_Framework_TestCase;
use Something\Company;
use Something\UserCreated;
/**
* @covers \Something\UserCreated
* @group todo
*/
class UserCreatedTest extends PHPUnit_Framework_TestCase
{
    /** @var string */
    private $name;
    /** @var int */
    private $age;
    /** @var float */
    private $height;
    /** @var  */
    private $about;
    /** @var Company */
    private $company;
    /** @var UserCreated */
    private $userCreated;
    public function setUp()
    {
        $this->name = 'name';
        $this->age = 1;
        $this->height = 2.0;
        $this->about = 'about';
        $this->company = Mockery::mock(Company::class);
        $this->userCreated = new UserCreated($this->name, $this->age, $this->height, $this->about, $this->company);
    }
    public function testGetName()
    {
        self::assertEquals($this->name, $this->userCreated->getName());
    }
    public function testGetAge()
    {
        self::assertEquals($this->age, $this->userCreated->getAge());
    }
    public function testGetHeight()
    {
        self::assertEquals($this->height, $this->userCreated->getHeight());
    }
    public function testGetAbout()
    {
        self::assertEquals($this->about, $this->userCreated->getAbout());
    }
    public function testGetCompany()
    {
        self::assertEquals($this->company, $this->userCreated->getCompany());
    }
}