{
    "name": "nulldevelopmenthr/nemesis",
    "description": "Nemesis",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "lib/"
        },
        "psr-4": {
            "": "src/",
            "spec\\": "spec/",
            "tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "friendsofphp/php-cs-fixer": "^2.4",
        "hanneskod/classtools": "^1.1",
        "league/tactician": "^1.0",
        "nette/php-generator": "^3",
        "nikic/php-parser": "^3.0",
        "roave/better-reflection": "^2.0",
        "sensio/generator-bundle": "^3.1",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/finder": "~3",
        "symfony/yaml": "^3.3",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "broadway/broadway": "^2.0",
        "broadway/read-model-elasticsearch": "^0.3.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "~2",
        "codeclimate/php-test-reporter": "^0.4.4",
        "henrikbjorn/phpspec-code-coverage": "~3",
        "humbug/humbug": "~1.0@dev",
        "mockery/mockery": "~1@dev",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "~2.1",
        "phpspec/phpspec": "~3",
        "phpstan/phpstan": "^0.8.0",
        "phpunit/phpunit": "~5",
        "ramsey/uuid": "^3.1",
        "satooshi/php-coveralls": "^1",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "replace": {
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.0"
        }
    },
    "scripts": {
    },
    "bin": ["bin/Nemesis"],
    "extra": {
    }
}
