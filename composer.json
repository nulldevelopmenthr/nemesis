{
    "name": "nulldevelopmenthr/Nemesis",
    "description": "Nemesis",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-0": {
            "": "lib/"
        },
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\": "spec/",
            "tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "broadway/read-model-elasticsearch": "^0.2.0",
        "hanneskod/classtools": "^1.1",
        "nikic/php-parser": "^3.0",
        "sensio/generator-bundle": "~3",
        "symfony/finder": "~3"
    },
    "require-dev": {
        "phpunit/phpunit":                          "~5",
        "mockery/mockery":                          "~0.9",
        "phpspec/phpspec":                          "~3",
        "henrikbjorn/phpspec-code-coverage":        "~3",
        "ciaranmcnulty/phpspec-typehintedmethods":  "~2",
        "phpmd/phpmd":                              "~2.1",
        "squizlabs/php_codesniffer":                "^3.0",
        "friendsofphp/php-cs-fixer":                "^2.0",
        "satooshi/php-coveralls":                   "^1",
        "codeclimate/php-test-reporter":            "^0.4.4",
        "broadway/broadway":                        "^1.0",
        "ramsey/uuid":                              "^3.1",
        "phpstan/phpstan":                          "^0.7.0",
        "phing/phing": "^2.16",
        "humbug/humbug": "~1.0@dev"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.0"
        }
    },
    "scripts": {
    },
    "bin": ["bin/Nemesis"],
    "extra": {
    }
}
