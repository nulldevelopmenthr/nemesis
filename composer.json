{
    "name": "nulldevelopmenthr/nemesis",
    "description": "Nemesis",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "comment": "Adding my own multiline fixers",
            "url": "https://github.com/msvrtan/PHP-CS-Fixer.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "src/",
            "spec\\": "spec/",
            "tests\\": "tests/",
            "Tests\\": "tests/",
            "MyVendor\\":"lib/src/MyVendor/",
            "spec\\MyVendor\\": "lib/spec/MyVendor/",
            "Tests\\MyVendor\\": "lib/tests/MyVendor/"
        }
    },
    "require": {
        "php": ">=7.1.0",
        "friendsofphp/php-cs-fixer": "dev-long-line-fixers",
        "hanneskod/classtools": "^1.1",
        "league/tactician": "^1.0",
        "myclabs/php-enum": "^1.5",
        "nette/php-generator": "^3",
        "nikic/php-parser": "^3.0",
        "roave/better-reflection": "^2.0",
        "symfony/config": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/finder": "~3",
        "symfony/yaml": "^3.3",
        "webmozart/assert": "^1.2"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "broadway/broadway": "^2.0",
        "broadway/read-model-elasticsearch": "^0.3.0",
        "ciaranmcnulty/phpspec-typehintedmethods": "^3.0",
        "codeclimate/php-test-reporter": "^0.4.4",
        "humbug/humbug": "dev-master",
        "leanphp/phpspec-code-coverage": "^4.0",
        "mockery/mockery": "~1@dev",
        "phing/phing": "^2.16",
        "phpmd/phpmd": "~2.1",
        "phpspec/phpspec": "^4.2",
        "phpstan/phpstan": "^0.9.1",
        "phpunit/phpunit": "^6.5",
        "ramsey/uuid": "^3.1",
        "satooshi/php-coveralls": "^1",
        "slevomat/coding-standard": "^4.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "replace": {
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php72": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "scripts": {
    },
    "bin": ["bin/Nemesis"],
    "extra": {
    }
}
